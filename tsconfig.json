{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@primer-io/app-framework": ["app-framework"]
    },
    "lib": [
      "ES2019"
    ],
    "module": "commonjs",
    "target": "ES2019",
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node"
  },
  "typeAcquisition": {
    "enable": true,
    "include": ["./typings.ts"]
  },
}